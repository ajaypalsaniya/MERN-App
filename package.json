{
	"name": "mernapp",
	"version": "1.0.0",
	"description": "MERN stack Shooping list",
	"main": "server.js",
	"scripts": {
		"client-install": "npm install --prefix client",
		"start": "npm server.js",
		"server": "nodemon server.js",
		"client": "cd client && npm start",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
