		"awesome",
		"app"
	],
	"author": "Ajay Palsaniya",
	"license": "MIT",
	"dependencies": {
		"body-parser": "^1.19.0",
		"concurrently": "^5.1.0",
		"express": "^4.17.3",
		"mongoose": "^5.9.7",
		"redux-thunk": "^2.3.0"
	}
}
